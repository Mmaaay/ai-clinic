{
  "name": "ai-clinic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write .",
    "typecheck": "npx tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "db:generate": "drizzle-kit generate --out ./drizzle --schema ./db/schema.ts",
    "db:migrate:dev": "drizzle-kit migrate dev --schema ./db/schema.ts",
    "db:push": "drizzle-kit migrate push --schema ./db/schema.ts",
    "db:reset": "tsx ./db/reset.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md,html}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"

    }
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-form": "^1.28.0",
    "@tanstack/react-form-devtools": "^0.2.13",
    "@tanstack/react-form-nextjs": "^1.28.0",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "lucide-react": "^0.562.0",
    "next": "^16.1.6",
    "npm-force-resolutions": "^0.0.10",
    "pdf-lib": "^1.17.1",
    "postgres": "^3.4.8",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "resolutions": {
    "react-hook-form": "7.71.1"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "drizzle-kit": {
      "esbuild": "^0.25.0"
    }
  }
}
